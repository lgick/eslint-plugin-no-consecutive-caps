{
  "name": "eslint-plugin-no-consecutive-caps",
  "version": "1.1.8",
  "type": "module",
  "description": "An ESLint plugin to disallow consecutive uppercase letters in identifiers, often found in acronyms.",
  "homepage": "https://github.com/lgick/eslint-plugin-no-consecutive-caps#readme",
  "bugs": {
    "url": "https://github.com/lgick/eslint-plugin-no-consecutive-caps/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lgick/eslint-plugin-no-consecutive-caps.git"
  },
  "license": "MIT",
  "author": "lgick",
  "types": "dist/index.d.ts",
  "main": "index.js",
  "scripts": {
    "test": "mocha tests/**/*.js",
    "clean": "rm -rf dist",
    "build": "npm run clean && tsc",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "index.js",
    "lib/",
    "dist/"
  ],
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "caps",
    "camelcase",
    "style",
    "naming",
    "acronym"
  ],
  "peerDependencies": {
    "eslint": "^8.0.0 || ^9.0.0"
  },
  "devDependencies": {
    "@types/eslint": "^8.56.10",
    "mocha": "^10.4.0",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5"
  }
}
